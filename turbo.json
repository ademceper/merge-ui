{
  "$schema": "https://turborepo.com/schema.json",
  "ui": "tui",
  "globalEnv": [
    "NODE_ENV",
    "EXPO_PUBLIC_API_URL",
    "NEXT_PUBLIC_API_URL",
    "EXPO_PUBLIC_SENTRY_DSN",
    "NEXT_PUBLIC_SENTRY_DSN",
    "DATABASE_URL",
    "EXPO_PUBLIC_ENABLE_ANALYTICS",
    "NEXT_PUBLIC_ENABLE_ANALYTICS"
  ],
  "tasks": {
    "build": {
      "inputs": ["$TURBO_DEFAULT$", ".env*"],
      "outputs": ["dist/**", ".next/**", "!.next/cache/**"],
      "dependsOn": ["^build"]
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "lint": {},
    "clean": {
      "cache": false
    }
  }
}
